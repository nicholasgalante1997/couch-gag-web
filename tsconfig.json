{
    "compilerOptions": {
      "target": "es2016",
      "lib": ["es6", "dom", "dom.iterable"],
      "jsx": "react-jsx",
      "module": "esnext",
      "rootDir": ".",
      "moduleResolution": "bundler",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "resolveJsonModule": true,
      "declaration": true,
      "noEmit": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "allowJs": true,
      "checkJs": true,
      "types": ["./src/types/icons/index.d.ts",]
    },
    "include": ["src", "src/workers/prefetch.js"],
    "exclude": ["node_modules"]
  }
  