{
  "name": "@couch-gag/web",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "run-s clean:build clean:tmp browser sleepy cp:assets cp:workers",
    "clean:build": "rm -rf build",
    "clean:tmp": "rm -rf .sleepy",
    "cp:assets": "cp -R src/styles/*.css src/styles/*.ttf node_modules/heller-2-lite/build/css/*.css assets/* build/",
    "cp:workers": "cp -R src/workers/* build",
    "dev:browse": "DEVELOPMENT_PAGE_INDEXING_KEY=browse webpack serve --config webpack/webpack.dev.js",
    "dev:lp": "DEVELOPMENT_PAGE_INDEXING_KEY=home webpack serve --config webpack/webpack.dev.js",
    "dev:story": "DEVELOPMENT_PAGE_INDEXING_KEY=story webpack serve --config webpack/webpack.dev.js",
    "prettier": "prettier --write src webpack",
    "lint": "eslint --fix \"{src,webpack,workers}/**/*.{ts,tsx,js}\" ",
    "browser": "webpack --config webpack/webpack.prod.js",
    "sleepy": "webpack --config webpack/webpack.node.js && node .sleepy/sleepy.cjs && rm -rf .sleepy",
    "exports": "node esbuild.config.js",
    "start": "npx serve build",
    "storybook": "storybook dev -p 6006",
    "build:md": "node scripts/build-md.mjs",
    "build-storybook": "storybook build",
    "test": "jest --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@9.1.2",
  "dependencies": {
    "@iconscout/react-unicons": "^2.0.2",
    "@vercel/analytics": "^1.0.1",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "heller-2-lite": "^0.0.22",
    "heller-2-react": "^0.0.9",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "pino": "^8.14.1",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^6.0.3",
    "react-sleepy": "^0.0.12",
    "sleepydogs": "^1.0.7",
    "uuid": "^9.0.0",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/core": "^1.7.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^20.14.13",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "babel-loader": "^9.1.3",
    "bundle-stats-webpack-plugin": "^4.4.2",
    "css-loader": "^7.1.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "json-loader": "^0.5.7",
    "prettier": "^2.8.8",
    "storybook": "^7.0.24",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
