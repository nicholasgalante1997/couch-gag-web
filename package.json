  "main": "index.js",
  "scripts": {
    "w": "jest --watch",
    "x": "npm-run-all build local-prod-start",
    "y": "npm-run-all lint format",
    "local-prod-start": "npx serve build",
    "test": "jest --passWithNoTests",
    "cp:assets": "cp -R src/styles/*.css src/styles/*.ttf assets/* build/",
    "clean:build": "rm -rf build",
    "storybook": "storybook dev -p 6006",
    "dev:lp": "webpack serve --config dev-configs/landing.webpack.js",
    "dev:story": "webpack serve --config dev-configs/story.webpack.js",
    "dev:browse": "webpack serve --config dev-configs/browse.webpack.js",
    "dev:contribute": "webpack serve --config dev-configs/contribute.webpack.js",
    "build-storybook": "storybook build",
    "format": "prettier --write src workers",
    "lint": "eslint --fix \"{src,workers}/**/*.{ts,tsx,js}\" ",
    "pkg:entry": "node .build-process/build-entry-points.js",
    "pkg:js": "webpack --config client.webpack.js",
    "pkg:ts-script": "webpack --config ssg.webpack.js",
    "pkg:html": "node .build-process/build-app.js",
    "pkg:metadata": "node build-md.mjs",
    "clean:tmp": "rm -rf .build-process",
    "cp:workers": "cp -R workers/* build",
    "build": "npm-run-all clean:build pkg:ts-script pkg:entry pkg:js pkg:html clean:tmp cp:assets cp:workers"
  },
